{
    "name": "instaclick/php-webdriver",
    "type": "library",
    "description": "PHP WebDriver for Selenium 2",
    "keywords": [
        "selenium",
        "webdriver",
        "webtest",
        "browser",
        "test",
        "automation"
    ],
    "homepage": "http://instaclick.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Justin Bishop",
            "email": "jubishop@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Anthon Pang",
            "email": "apang@softwaredevelopment.ca",
            "role": "Fork Maintainer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*"
    },
    "require-dev": {
        "php": ">=7.2",
        "phpunit/phpunit": "^8.5 || ^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "WebDriver": "lib/"
       }
    },
    "autoload-dev": {
        "psr-0": {
            "Test": "test/"
       }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
